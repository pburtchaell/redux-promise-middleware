{
  "stage": 0,
  "loose": [
    "es6.modules"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": [
          {
            "target": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          },
          {
            "target": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }
        ]
      }
    },
    "production": {
      "plugins": [
        "react-display-name"
      ]
    }
  }
}
